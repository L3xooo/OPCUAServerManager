{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "plugin:react/recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "indent": ["error", 2],
    "react/prop-types": "off",
    "semi": ["error", "always"],
    "quotes": ["error", "double"],
    "keyword-spacing": ["error", { "after": true }],
    "no-unused-vars": "error"
  }
}
